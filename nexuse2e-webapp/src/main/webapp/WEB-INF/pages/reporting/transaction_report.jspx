                                <form:option value="2">Queued</form:option>
                                <form:option value="-1">Failed</form:option>
                                <form:option value="4">Stopped</form:option>
                                <form:option value="1">Retrying</form:option>
                                <form:option value="3">Sent</form:option>
                                <form:option value="0">Unknown</form:option>
                                <form:option value="1,2">#active#</form:option>
                                <form:option value="-1,3,4">#inactive#</form:option>
                            </form:select>
                        </c:when>
                        <c:otherwise>
                            <form:select onchange="javascript: disableLinks();" styleClass="fixedsize" path="status">
                                <form:option value="allConversations">&#160;</form:option>
                                <form:option value="2">Processing</form:option>
                                <form:option value="9">Completed</form:option>
                                <form:option value="1">Created</form:option>
                                <form:option value="-1">Error</form:option>
                                <form:option value="4">Idle</form:option>
                                <form:option value="0">Unknown</form:option>
                                <form:option value="1,2,3,5,6,7,8">#active#</form:option>
                                <form:option value="4,9,-1">#inactive#</form:option>
                            </form:select>
