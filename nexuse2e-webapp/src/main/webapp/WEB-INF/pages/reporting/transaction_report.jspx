                            </c:if>
                        </tr>
                        <c:forEach var="message" items="${collection}">
                            <tr>
                                <td class="NEXUSValue">
                                    <c:if test="${reportingSettingsForm.messColSelect and reportingPropertiesForm.type != 'purge'}">
                                        <form:checkbox path="selected" value="${message.participantId}|${message.choreographyId}|${message.conversationId}|${message.messageId}" />
                                    </c:if>
                                </td>
                                <td class="NEXUSValue">
                                    <nexus:link styleClass="NexusLink" href="MessageView.do?mId=${message.messageId}&amp;convId=${message.conversationId}&amp;chorId=${message.choreographyId}&amp;partnerId=${message.participantId}">
                                        ${message.messageId}
                                    </nexus:link>
                                </td>
                                <td class="NEXUSValue">
                                    <nexus:link styleClass="NexusLink" href="ConversationView.do?convId=${message.nxConversationId}">
                                        ${message.conversationId}
                                    </nexus:link>
                                </td>
                                <c:if test="${reportingSettingsForm.messColParticipantId}">
                                    <td class="NEXUSValue">
                                        ${message.participantId}
                                    </td>
                                </c:if>
                                <c:if test="${reportingSettingsForm.messColStatus}">
                                    <td class="NEXUSValue">
                                        ${message.status}
                                    </td>
                                </c:if>
                                <c:if test="${reportingSettingsForm.messColBackendStatus}">
                                    <td class="NEXUSValue">
                                        ${message.backendStatus}
                                    </td>
                                </c:if>
                                <c:if test="${reportingSettingsForm.messColType}">
                                    <td class="NEXUSValue">
                                        ${message.type}
                                    </td>
                                </c:if>
                                <c:if test="${reportingSettingsForm.messColAction}">
                                    <td class="NEXUSValue">
                                        ${message.action}
                                    </td>
                                </c:if>
                                <c:if test="${reportingSettingsForm.messColCreated}">
                                    <td class="NEXUSValue">
                                        ${message.createdDate}
                                    </td>
                                </c:if>
                                <c:if test="${reportingSettingsForm.messColTurnaround}">
                                    <td class="NEXUSValue">
                                        ${message.turnaroundTime}
                                    </td>
                                </c:if>
                            </tr>
                        </c:forEach>
                    </table>
                    <c:if test="${reportingSettingsForm.messColSelect and reportingPropertiesForm.type != 'purge'}">
                        <table class="NEXUS_BUTTON_TABLE" width="100%">
                            <tr>
                                <td class="BUTTON_LEFT" width="75px">
                                    <a href="#" id="startLink" onClick="javascript: selectAll(true);return false;" class="NexusLink">
                                        <nobr>Select all</nobr>
                                    </a>
                                    <br />
                                    <a href="#" id="startLink" onClick="javascript: selectAll(false)" class="NexusLink">
                                        <nobr>Deselect all</nobr>
                                    </a>
                                </td>
                                <td class="BUTTON_RIGHT">
                                    <nobr>
                                        <nexus:submit onClick="document.forms['reportingPropertiesForm'].command.value='requeue';">
                                            <img src="images/icons/arrow_redo.png" name="clearButton" class="button"/>
                                            Re-Queue
                                        </nexus:submit>
                                    </nobr>
                                </td>
                                <td class="BUTTON_RIGHT">
                                    <nobr>
                                        <nexus:submit onClick="document.forms['reportingPropertiesForm'].command.value='stop';">
                                            <img src="images/icons/arrow_rotate_anticlockwise.png" name="clearButton" class="button"/>
                                            Stop
                                        </nexus:submit>
                                    </nobr>
                                </td>
                            </tr>
                        </table>
                    </c:if>
                </c:if>
            </c:when>
            <c:otherwise>
                <c:if test="${not empty collection}">
                    <table class="NEXUS_TABLE" width="100%">
                        <tr>
                            <th class="NEXUSSection"></th>
                            <c:if test="${reportingSettingsForm.convColChorId}">
                                <th class="NEXUSSection">Choreography ID</th>
                            </c:if>
                            <th class="NEXUSSection">Conversation ID</th>
                            <c:if test="${reportingSettingsForm.convColPartId}">
                                <th class="NEXUSSection">Participant ID</th>
                            </c:if>
                            <c:if test="${reportingSettingsForm.convColStatus}">
                                <th class="NEXUSSection">Status</th>
                            </c:if>
                            <c:if test="${reportingSettingsForm.convColAction}">
                                <th class="NEXUSSection">Current Action</th>
                            </c:if>
                            <c:if test="${reportingSettingsForm.convColCreated}">
                                <th class="NEXUSSection">Date Created</th>
                            </c:if>
                            <c:if test="${reportingSettingsForm.convColTurnaround}">
                                <th class="NEXUSSection">Turnaround Time</th>
                            </c:if>
                        </tr>
                        <c:forEach var="conv" items="${collection}">
                            <tr>
                                <td class="NEXUSValue">
                                    <c:if test="${reportingSettingsForm.convColSelect and reportingPropertiesForm.type != 'purge'}">
                                        <form:checkbox path="selected" value="${conv.participantId}|${conv.choreographyId}|${conv.conversationId}" />
                                    </c:if>
                                </td>
                                <c:if test="${reportingSettingsForm.convColChorId}">
                                    <td class="NEXUSValue">
                                        ${conv.choreographyId}
                                    </td>
                                </c:if>
                                <td class="NEXUSValue">
                                    <nexus:link styleClass="NexusLink" href="ConversationView.do?convId=${conv.nxConversationId}">
                                        ${conv.conversationId}
                                    </nexus:link>
                                </td>
                                <c:if test="${reportingSettingsForm.convColPartId}">
                                    <td class="NEXUSValue">
                                        ${conv.participantId}
                                    </td>
                                </c:if>
                                <c:if test="${reportingSettingsForm.convColStatus}">
                                    <td class="NEXUSValue">
                                        ${conv.status}
                                    </td>
                                </c:if>
                                <c:if test="${reportingSettingsForm.convColAction}">
                                    <td class="NEXUSValue">
                                        ${conv.action}
                                    </td>
                                </c:if>
                                <c:if test="${reportingSettingsForm.convColCreated}">
                                    <td class="NEXUSValue">
                                        ${conv.createdDate}
                                    </td>
                                </c:if>
                                <c:if test="${reportingSettingsForm.convColTurnaround}">
                                    <td class="NEXUSValue">
                                        ${conv.turnaroundTime}
                                    </td>
                                </c:if>
                            </tr>
                        </c:forEach>
                    </table>
                    <c:if test="${reportingSettingsForm.convColSelect and reportingPropertiesForm.type != 'purge'}">
                        <table class="NEXUS_BUTTON_TABLE" width="100%" border="1">
                            <tr>
                                <td class="BUTTON_LEFT" width="75px">
                                    <a href="#" id="startLink" onClick="javascript: selectAll(true);return false;" class="NexusLink">
                                        <nobr>Select all</nobr>
                                    </a>
                                    <br />
                                    <a href="#" id="startLink" onClick="javascript: selectAll(false)" class="NexusLink">
                                        <nobr>Deselect all</nobr>
                                    </a>
                                </td>
                                <td  class="BUTTON_RIGHT">
                                    <nexus:submit onClick="document.forms['reportingPropertiesForm'].command.value='delete';">
                                    <img src="images/icons/delete.png" name="clearButton" class="button"/>
                                    Delete</nexus:submit>
                                </td>
                            </tr>
                        </table>
                    </c:if>
                </c:if>
            </c:otherwise>
        </c:choose>
    </form:form>
</jsp:root>