                    <form:select path="choreographyId" onchange="javascript: disableLinks();" styleClass="fixedsize">
                        <form:option value="">&#160;</form:option>
                        <form:options items="${reportingPropertiesForm.choreographyIds}"/>
                    </form:select>
                </td>
            </tr>
            <tr>
                <td class="NEXUSValue">Participant ID</td>
