                    </c:choose>
                </td>
                <td id="messageIdText" class="NEXUSValue">
                    <form:checkbox onchange="javascript: disableLinks();" path="messageEnabled" value="true" label=" Message ID" />
                </td>
                <td class="NEXUSValue">
                    <form:input onchange="javascript: disableLinks();" styleClass="fixedsize" path="messageId"/>
                </td>
            </tr>
    
            <tr>
                <td class="NEXUSValue">
                    <form:checkbox onchange="disableLinks();" path="startEnabled" value="true" label=" Start Date" />
                </td>
