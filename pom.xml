   </resources>
   <plugins>
     <plugin>
       <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-compiler-plugin</artifactId>
       <configuration>
         <source>1.5</source>
         <target>1.5</target>
       </configuration>
     </plugin>
     <plugin>
       <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-war-plugin</artifactId>
       <version>2.0.2</version>
       <extensions>true</extensions>
       <configuration>
         <archiveClasses>true</archiveClasses>
         <archive>
           <manifest>
             <mainClass>org.nexuse2e.Version</mainClass>
           </manifest>
         </archive>
         <warName>NEXUSe2e</warName>
         <warSourceExcludes>
           **/stax-api-1.0-2.jar,
           **/wstx-asl-3.2.1.jar,
           **/icu4j-2.6.1.jar,
           **/axis2-kernel-1.3.jar,
           **/XmlSchema-1.1.jar,
           **/spring*2.0.0.jar,
           **/spring-beans*.jar,
           **/spring-context*.jar,
           **/spring-core*.jar,
           **/spring-web*.jar,
           **/ant-1.5.jar,
           **/antlr-2.7.2.jar,
           **/cglib-2.1.jar,
           **/jsp*,
           **/servlet-api*,
           **/javax.xml.bind-jaxb-api-2.1.jar,
           **/xml-apis*,
           **/geronimo-javamail_*.jar,
           **/geronimo-servlet_*.jar,
           **/geronimo-activation_*.jar,
           **/geronimo-stax-api_*.jar,
           **/jaxws-api-*.jar,
           **/xercesImpl-2.6.2.jar,
           WEB-INF/classes/tldCache.ser,
           WEB-INF/classes/org/**,
           WEB-INF/config/database/derby/**,
           /WEB-INF/config/bean.properties,
           /WEB-INF/config/database.properties
         </warSourceExcludes>
         <webResources>
           <resource>
             <directory>src/main/war_resources</directory>
             <includes>
               <include>**/*.properties</include>
             </includes>
           </resource>
         </webResources>
         <manifest>
           <addDefaultImplementationEntries>true</addDefaultImplementationEntries>
         </manifest>
         <archive>
           <manifestEntries>
             <Application-Name>${project.name}</Application-Name>
             <Application-Artifact-Id>${project.artifactId}</Application-Artifact-Id>
             <Application-Group-Id>${project.groupId}</Application-Group-Id>
             <Implementation-Svn-Revision>${env.SVN_REVISION}</Implementation-Svn-Revision>
             <Implementation-Version>${project.version}</Implementation-Version>
             <Implementation-Build-Date>${env.BUILD_ID}</Implementation-Build-Date>
           </manifestEntries>
         </archive>
       </configuration>
     </plugin>
     <plugin>
       <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-site-plugin</artifactId>
       <configuration>
         <locales>en</locales>
         <templateDirectory>${basedir}</templateDirectory>
         <template>nexuse2e-site.vm</template>
       </configuration>
     </plugin>
     <plugin>
       <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-jar-plugin</artifactId>
       <configuration>
         <finalName>nexuse2e</finalName>
         <archive>
           <index>true</index>
           <manifest>
             <manifest>
               <addClasspath>true</addClasspath>
               <mainClass>org.nexuse2e.Version</mainClass>
             </manifest>
           </manifest>
           <manifestEntries>
             <Application-Name>${project.name}</Application-Name>
             <Application-Artifact-Id>${project.artifactId}</Application-Artifact-Id>
             <Application-Group-Id>${project.groupId}</Application-Group-Id>
             <Implementation-Svn-Revision>${env.SVN_REVISION}</Implementation-Svn-Revision>
             <Implementation-Version>${project.version}</Implementation-Version>
             <Implementation-Build-Date>${env.BUILD_ID}</Implementation-Build-Date>
           </manifestEntries>
         </archive>
       </configuration>
     </plugin>
     <plugin>
       <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-javadoc-plugin</artifactId>
       <version>2.1</version>
       <configuration>
         <javadocDirectory>${basedir}/src/main/javadoc</javadocDirectory>
         <overview>${basedir}/src/main/javadoc/overview.html</overview>
       </configuration>
     </plugin>
     <plugin>
       <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-antrun-plugin</artifactId>
       <dependencies>

         <!-- runtime dependency for the antrun plugin. It brings tools.jar
           into the plugins execution space so the 'rmic' compiler can be called from
           Ant's <rmic/> task. This is ALL you have to do!
         -->
         <dependency>
           <groupId>sun</groupId>
           <artifactId>tools</artifactId>
           <version>1.0</version>
           <scope>system</scope>
           <!-- systemPath>${java.home}/../lib/tools.jar</systemPath -->
           <systemPath>${tools.jar}</systemPath>
         </dependency>

       </dependencies>
       <executions>
         <execution>
           <id>generate-sources</id>
           <phase>generate-sources</phase>
           <configuration>
             <tasks>
               <ant antfile="${basedir}/nexuse2e-build.xml">
                 <target name="VersionNumber" />
               </ant>
             </tasks>
           </configuration>
           <goals>
             <goal>run</goal>
           </goals>
         </execution>
         <execution>
           <id>build-testclient</id>
           <phase>package</phase>
           <configuration>
             <tasks>
               <ant antfile="${basedir}/package-testclient.xml"/>
             </tasks>
           </configuration>
           <goals>
             <goal>run</goal>
           </goals>
         </execution>
         <execution>
           <id>build-patches</id>
           <phase>package</phase>
           <configuration>
             <tasks>
               <ant antfile="${basedir}/package-patches.xml"/>
             </tasks>
           </configuration>
           <goals>
             <goal>run</goal>
           </goals>
         </execution>
         <execution>
           <id>compile</id>
           <phase>compile</phase>
           <configuration>
             <tasks>
               <rmic classname="org.nexuse2e.integration.NEXUSe2eInterfaceImpl" base="${basedir}/target/classes" />
             </tasks>
           </configuration>
           <goals>
             <goal>run</goal>
           </goals>
         </execution>
       </executions>
     </plugin>
     <plugin>
       <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-surefire-plugin</artifactId>
       <configuration>
         <excludes>
           <exclude>**/TestMessageSenderService.java</exclude>
           <exclude>**/TestMessageSenderPipelet.java</exclude>
         </excludes>
       </configuration>
     </plugin>
     <plugin>
       <groupId>org.mortbay.jetty</groupId>
       <artifactId>maven-jetty-plugin</artifactId>
       <configuration>
         <contextPath>/NEXUSe2e</contextPath>
         <connectors>
           <connector implementation="org.mortbay.jetty.nio.SelectChannelConnector">
             <port>8080</port>
             <maxIdleTime>60000</maxIdleTime>
           </connector>
         </connectors>
       </configuration>
     </plugin>
