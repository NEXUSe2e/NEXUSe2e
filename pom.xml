           <version>4.4</version>
           <scope>runtime</scope>
         </dependency>
       </dependencies>
       <configuration>
         <xincludeSupported>true</xincludeSupported>
         <includes>NEXUSe2e_4_Manual.xml</includes>
         <chunkedOutput>true</chunkedOutput>
       </configuration>
     </plugin>
      -->
     <!-- 
     <plugin>
       <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-resources-plugin</artifactId>
       <configuration></configuration>
     </plugin>
      -->

     <!-- Use wsdl2java in order to generate source from WSDL documents -->
     <plugin>
       <groupId>org.apache.cxf</groupId>
       <artifactId>cxf-codegen-plugin</artifactId>
       <version>2.1.2</version>
       <executions>
         <execution>
           <id>generate-sources</id>
           <phase>generate-sources</phase>
           <configuration>
             <sourceRoot>${basedir}/src/main/java</sourceRoot>
             <wsdlOptions>
               <wsdlOption>
                 <wsdl>${basedir}/src/main/resources/org/nexuse2e/integration/AgGateway.wsdl</wsdl>
                 <extraargs>
                   <extraarg>-p</extraarg>
                   <extraarg>org.nexuse2e.service.ws.aggateway.wsdl</extraarg>
                   <extraarg>-wsdlLocation</extraarg>
                   <extraarg>classpath:org/nexuse2e/integration/AgGateway.wsdl</extraarg>
                 </extraargs>
               </wsdlOption>
             </wsdlOptions>
           </configuration>
           <goals>
             <goal>wsdl2java</goal>
           </goals>
         </execution>
       </executions>
     </plugin>

   </plugins>
 </build>
 <profiles>
   <!-- interoperation profiles -->
   <profile>
     <id>Windows.profile</id>
     <properties>
       <tools.jar>${java.home}/../lib/tools.jar</tools.jar>
     </properties>
     <activation>
       <os>
         <family>windows</family>
       </os>
     </activation>
   </profile>
   <profile>
     <id>Unix.profile</id>
     <properties>
       <tools.jar>${java.home}/../lib/tools.jar</tools.jar>
     </properties>
     <activation>
       <os>
         <family>unix</family>
       </os>
     </activation>
   </profile>
   <profile>
     <id>Mac.profile</id>
     <properties>
       <tools.jar>/System/Library/Frameworks/JavaVM.framework/Versions/1.5.0/Classes/classes.jar</tools.jar>
     </properties>
     <activation>
       <os>
         <family>mac</family>
       </os>
     </activation>
   </profile>
   <!-- dependency related -->
   <profile>
     <id>with-reporting-module</id>
     <activation>
         <activeByDefault>false</activeByDefault>
     </activation>
     <build>
         <plugins>
             <plugin>
                 <groupId>org.apache.maven.plugins</groupId>
                 <artifactId>maven-dependency-plugin</artifactId>
                 <executions>
                     <execution>
                         <id>gather-reporting-module</id>
                         <phase>generate-resources</phase>
                         <goals>
                             <goal>unpack</goal>
                         </goals>
                         <configuration>
                             <artifactItems>
                                 <artifactItem>
                                     <groupId>org.nexuse2e</groupId>
                                     <artifactId>nexuse2e-reporting</artifactId>
                                     <version>1.0</version>
                                     <type>jar</type>
                                 </artifactItem>
                             </artifactItems>
                             <outputDirectory>${project.build.directory}/reporting</outputDirectory>
                             <overWriteReleases>false</overWriteReleases>
                             <overWriteSnapshots>true</overWriteSnapshots>
                         </configuration>
                     </execution>
                 </executions>
             </plugin>
             <plugin>
                   <groupId>org.apache.maven.plugins</groupId>
                   <artifactId>maven-war-plugin</artifactId>
                   <extensions>true</extensions>
                   <configuration>
                     <webResources>
                         <resource>
                             <directory>${project.build.directory}/reporting/platform</directory>
                             <includes>
                                 <include>**</include>
                             </includes>
                             <targetPath>WEB-INF/platform</targetPath>
                             <filtering>false</filtering>
                         </resource>
                     </webResources>
                 </configuration>
             </plugin>
         </plugins>
     </build>
   </profile>
 </profiles>
</project>