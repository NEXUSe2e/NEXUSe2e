<project name="nexuse2e-server.rmi" default="rmi" basedir=".">
	<property file="nexuse2e-build.properties" />

	<target name="init" description="Initialize properties etc.">
		<echo message="Init RMIC..." />
	</target>

	<target name="rmi" description="Copy the nexuse2e.jar file">
		<echo message="Running RMIC..." />
		<rmic classname="org.nexuse2e.integration.NEXUSe2eInterfaceImpl" base="${basedir}/target/classes" />
	</target>

</project>