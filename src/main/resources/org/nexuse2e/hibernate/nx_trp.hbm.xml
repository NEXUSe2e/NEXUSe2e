<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.nexuse2e.pojo.TRPPojo" table="nx_trp" lazy="false">
        <comment></comment>
        <id name="nxTRPId" type="int">
            <column name="nx_trp_id" sql-type="integer"/>
            <generator class="native" />
        </id>
        <property name="protocol" type="string">
            <column name="protocol" length="64" not-null="true">
                <comment></comment>
            </column>
        </property>
        <property name="version" type="string">
            <column name="version" length="64" not-null="true">
                <comment></comment>
            </column>
        </property>
        <property name="transport" type="string">
            <column name="transport" length="64" not-null="true">
                <comment></comment>
            </column>
        </property>
        <property name="adapterClassName" type="string">
            <column name="adapter_class" length="255" not-null="false">
                <comment></comment>
            </column>
        </property>
        <property name="createdDate" type="timestamp">
            <column name="created_date" not-null="true">
                <comment></comment>
            </column>
        </property>
        <property name="modifiedDate" type="timestamp">
            <column name="modified_date" not-null="true">
                <comment></comment>
            </column>
        </property>
        <property name="modifiedNxUserId" type="int">
            <column name="modified_nx_user_id" not-null="true" sql-type="integer">
                <comment></comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
