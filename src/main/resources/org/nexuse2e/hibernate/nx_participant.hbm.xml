<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="all">
    <class name="org.nexuse2e.pojo.ParticipantPojo" table="nx_participant" lazy="false">
        <comment></comment>
        <id name="nxParticipantId" type="int">
            <column name="nx_participant_id" sql-type="integer"/>
            <generator class="native" />
        </id>
        
        <many-to-one foreign-key="fk__part__certificate_id" name="localCertificate" column="nx_local_certificate_id" class="org.nexuse2e.pojo.CertificatePojo" not-null="false" cascade="none" index="ix_participant_1"/>
        
        <many-to-one foreign-key="fk__part__partner_id" name="partner" column="nx_partner_id" class="org.nexuse2e.pojo.PartnerPojo" not-null="true" cascade="none" index="ix_participant_2"/>
        
        <many-to-one foreign-key="fk__part__choreography_id" name="choreography" column="nx_choreography_id" class="org.nexuse2e.pojo.ChoreographyPojo" not-null="true" cascade="none" index="ix_participant_3"/>
        
        <many-to-one foreign-key="fk__part__local_partner_id" name="localPartner" column="nx_local_partner_id" class="org.nexuse2e.pojo.PartnerPojo" not-null="true" cascade="none" index="ix_participant_4"/>
        
        <many-to-one foreign-key="fk__part__connection_id" name="connection" column="nx_connection_id" class="org.nexuse2e.pojo.ConnectionPojo" not-null="true" cascade="none" index="ix_participant_5"/>
        
        <property name="createdDate" type="timestamp">
            <column name="created_date" not-null="true">
                <comment></comment>
            </column>
        </property>
        
        <property name="modifiedDate" type="timestamp">
            <column name="modified_date" not-null="true">
                <comment></comment>
            </column>
        </property>
        
        <property name="modifiedNxUserId" type="int">
            <column name="modified_nx_user_id" not-null="true" sql-type="integer">
                <comment></comment>
            </column>
        </property>
        
        <property name="description" type="string">
            <column name="description" length="64">
                <comment></comment>
            </column>
        </property>
        
        
    </class>
</hibernate-mapping>
