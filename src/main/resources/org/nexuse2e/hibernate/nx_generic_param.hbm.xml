<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.nexuse2e.pojo.GenericParamPojo" table="nx_generic_param" lazy="false">
        <comment></comment>
        <id name="nxGenericParamId" type="int">
            <column name="nx_generic_param_id" sql-type="integer"/>
            <generator class="native" />
        </id>
        
        <property name="category" type="string">
            <column name="category" length="128" not-null="true">
                <comment></comment>
            </column>
        </property>
        <property name="tag" type="string">
            <column name="param_tag" length="128" not-null="false">
                <comment></comment>
            </column>
        </property>
        
        <property name="createdDate" type="timestamp">
            <column name="created_date" not-null="true">
                <comment></comment>
            </column>
        </property>
        
        <property name="modifiedDate" type="timestamp">
            <column name="modified_date" not-null="true">
                <comment></comment>
            </column>
        </property>
        
        <property name="modifiedNxUserId" type="int">
            <column name="modified_nx_user_id" not-null="true" sql-type="integer">
                <comment></comment>
            </column>
        </property>
        
        <property name="paramName" type="string">
            <column name="param_name" length="64" not-null="true">
                <comment></comment>
            </column>
        </property>
        
        <property name="label" type="string">
            <column name="param_label" length="64">
                <comment></comment>
            </column>
        </property>
        <property name="value" type="string">
            <column name="param_value" length="64" not-null="true">
                <comment></comment>
            </column>
        </property>
        <property name="sequenceNumber" type="int">
            <column name="sequence_number" sql-type="integer">
                <comment></comment>
            </column>
        </property>
        
    </class>
</hibernate-mapping>
