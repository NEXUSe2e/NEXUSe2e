<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.nexuse2e.pojo.MappingPojo" table="nx_mapping" lazy="false">
        <comment></comment>
        <id name="nxMappingId" type="int">
            <column name="nx_mapping_id" sql-type="integer"/>
            <generator class="native" />
        </id>
        
        <property name="category" type="string">
            <column name="category" length="128" not-null="true">
                <comment></comment>
            </column>
        </property>
        <property name="createdDate" type="timestamp">
            <column name="created_date" not-null="true">
                <comment></comment>
            </column>
        </property>
        <property name="modifiedDate" type="timestamp">
            <column name="modified_date" not-null="true">
                <comment></comment>
            </column>
        </property>
        
        <property name="modifiedNxUserId" type="int">
            <column name="modified_nx_user_id" not-null="true" sql-type="integer">
                <comment></comment>
            </column>
        </property>
        
        <property name="leftType" type="int">
            <column name="left_type" not-null="true" sql-type="integer">
                <comment></comment>
            </column>
        </property>
        <property name="leftValue" type="string">
            <column name="left_value" length="128" not-null="false">
                <comment></comment>
            </column>
        </property>
        
        <property name="rightType" type="int">
            <column name="right_type" not-null="true" sql-type="integer">
                <comment></comment>
            </column>
        </property>
        <property name="rightValue" type="string">
            <column name="right_value" length="128" not-null="false">
                <comment></comment>
            </column>
        </property>
        
    </class>
</hibernate-mapping>
