<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <!-- START SNIPPET: xfire -->
    <bean class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
        <property name="urlMap">
            <map>
                <entry key="/NEXUSe2eInterface">
                    <ref bean="NEXUSe2eInterface"/>
                </entry>
                <entry key="/EngineStatusInterface">
                    <ref bean="EngineStatusInterface"/>
                </entry>
                <!-- 
                <entry key="/TrafficLogger">
                    <bean class="org.nexuse2e.test.RequestLogger"/>
                </entry>
                <entry key="/BackendDeliveryInterface">
                  <ref bean="BackendDeliveryInterface"/>
                </entry>
                 -->
            </map>
        </property>
    </bean>
    
    <!-- Declare a parent bean with all properties common to both services -->
    <bean id="NEXUSe2eInterface" class="org.codehaus.xfire.spring.remoting.XFireExporter">
        <property name="serviceFactory">
            <ref bean="xfire.serviceFactory"/>
        </property>
        <property name="xfire">
            <ref bean="xfire"/>
        </property>
        <property name="serviceBean">
            <ref bean="nexusE2EInterfaceBean"/>
        </property>
        <property name="serviceClass">
            <value>org.nexuse2e.integration.NEXUSe2eInterface</value>
        </property>
    </bean>
    
    <!-- Declare a parent bean with all properties common to both services -->
    <bean id="EngineStatusInterface" class="org.codehaus.xfire.spring.remoting.XFireExporter">
        <property name="serviceFactory">
            <ref bean="xfire.serviceFactory"/>
        </property>
        <property name="xfire">
            <ref bean="xfire"/>
        </property>
        <property name="serviceBean">
            <ref bean="engineStatusInterfaceBean"/>
        </property>
        <property name="serviceClass">
            <value>org.nexuse2e.integration.EngineStatusInterface</value>
        </property>
    </bean>
    <!-- END SNIPPET: xfire -->

