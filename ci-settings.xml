<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    
        
    <profiles>
        <profile>
            <id>solutions-direkt</id>
            <properties>
                <distribution.release.repoId>so-releases</distribution.release.repoId>
                <distribution.release.repoName>solutions direkt Releases</distribution.release.repoName>
                <distribution.release.repoUrl>https://dev.direkt-gruppe.de/nexus/content/repositories/so-releases</distribution.release.repoUrl>
                <distribution.snapshot.repoId>so-snapshots</distribution.snapshot.repoId>
                <distribution.snapshot.repoName>solutions direkt Snapshots</distribution.snapshot.repoName>
                <distribution.snapshot.repoUrl>https://dev.direkt-gruppe.de/nexus/content/repositories/so-snapshots</distribution.snapshot.repoUrl>
            </properties>
            <repositories>
                <repository>
                    <id>codehaus-snapshots</id>
                    <name>Maven Snapshots Repository of Codehaus</name>
                    <layout>default</layout>
                    <url>http://snapshots.repository.codehaus.org</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
            
        </profile>
        
    </profiles>
    <activeProfiles>
        <activeProfile>solutions-direkt</activeProfile>
    </activeProfiles>
</settings>
